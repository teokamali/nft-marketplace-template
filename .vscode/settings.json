{
    "eslintConfig": {
      "extends": ["react-app", "shared-config"],
      "rules": {
        "additional-rule": "warn"
      },
      "overrides": [
        {
          "files": ["**/*.ts?(x)"],
          "rules": {
            "additional-typescript-only-rule": "warn"
          }
        }
      ]
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true 
  }